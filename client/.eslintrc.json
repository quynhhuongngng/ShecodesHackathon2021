{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            1,
            {
              "extensions": [
                ".js",
                ".jsx"
              ]
            }
          ],
        "react/prop-types": "off",
        "max-len": 0,
        "react/no-array-index-key": 0,
        "no-unused-vars": 0,
        "jsx-a11y/anchor-is-valid": 0
    }
}
